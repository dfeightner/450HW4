@model Vehicle

<div class="row">
    <form asp-action="Delete">
        <input type="hidden" asp-for="VehicleId" />

        <div class="form-group">
            <label asp-for="Mileage" class="label-control"></label>
            <input asp-for="Mileage" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="DateReceived" class="label-control"></label>
            <input asp-for="DateReceived" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="DateSold" class="label-control"></label>
            <input asp-for="DateSold" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="VehicleStatus" class="label-control"></label>
            <input asp-for="VehicleStatus" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="VIN" class="label-control"></label>
            <input asp-for="VIN" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Make" class="label-control"></label>
            <input asp-for="Make" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Model" class="label-control"></label>
            <input asp-for="Model" disabled class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Year" class="label-control"></label>
            <input asp-for="Year" disabled class="form-control" />
        </div>

        <div class="form-group">
            <input type="submit" value="Delete Vehicle" class="btn btn-primary" />
        </div>

    </form>

</div>